{
  "name": "some-nodejs-proj",
  "private": true,
  "version": "0.1.0",
  "description": "some test proj",
  "main": "server.js",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@kubernetes/client-node": "^0.12.0",
    "@sap/xsenv": "^1.2.7",
    "bluebird": "^3.7.2",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "http-errors": "^1.7.3",
    "koa": "^2.12.1",
    "koa-bodyparser": "^4.3.0",
    "koa-passport": "^4.1.3",
    "koa-router": "^9.0.1",
    "lodash": "^4.17.19",
    "prom-client": "12.0.0",
    "request-promise": "^4.2.5",
    "statuses": "^2.0.0",
    "uuid": "^8.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "mocha": "^7.2.0",
    "mocha-junit-reporter": "^2.0.0",
    "mochawesome": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "npx": "^10.2.2",
    "nyc": "^15.1.0",
    "opn": "^6.0.0",
    "sinon": "^9.0.2",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "lint": "npx eslint . --ext .js",
    "lint+": "npx eslint . -f jslint-xml -o ./test/coverage/eslint.jslint.xml",
    "lint-ci": "npx eslint . --ignore-pattern node_modules/ -f jslint-xml -o ./test/coverage/eslint.jslint.xml || true",
    "report": "npx opener ./test/report/test-results.html &",
    "test": "npm run test-unit",
    "test+": "npx mocha test/**/*spec.js --reporter mochawesome --reporter-options reportDir=./test/report,reportFilename=test-results,inline=true",
    "test++": "npx run-s -c test+ report",
    "test-int": "npx env NODE_ENV=test mocha test/integration/*spec.js",
    "test-int+": "npx env NODE_ENV=test mocha test/integration/*spec.js --reporter mochawesome --reporter-options reportDir=./test/report/int",
    "test-unit": "npx env NODE_ENV=test mocha test/unit/*spec.js",
    "test-unit+": "npx env NODE_ENV=test mocha test/unit/*spec.js --reporter mochawesome --reporter-options reportDir=./test/report,reportFilename=test-results,inline=true",
    "test-unit-ci": "npx env mocha test/unit/*spec.js --reporter mocha-junit-reporter --reporter-options mochaFile=./test/report/TEST-unit.xml || true",
    "test-system": "npx env NODE_ENV=test mocha test/system/*spec.js",
    "test-system+": "npx env NODE_ENV=test mocha test/system/*spec.js --reporter mochawesome --reporter-options reportDir=./test/report,reportFilename=test-results,inline=true",
    "test-coverage": "nyc --reporter=text mocha test/unit/*spec.js",
    "test-coverage+": "nyc --reporter=lcov --report-dir=./test/report/coverage mocha test/**/*spec.js",
    "test-ci": "node -v; npm -v; which npx; npx mocha test/unit/*spec.js --reporter mochawesome --reporter-options reportDir=./test-report/report,reportFilename=test-report.html --exit",
    "nyc-report": "nyc report --reporter=lcov --report-dir=./test-report/coverage",
    "start": "node start.js"
  },
  "author": "Jayesh",
  "license": "UNLICENSED"
}
